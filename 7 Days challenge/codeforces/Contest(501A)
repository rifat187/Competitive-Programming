int main()
{
    //freopen("in.txt","r",stdin);
    ll t;
    ll a,b,c,d,x,y;
    cin>>a>>b>>c>>d;


    x = max((3*a/10),a-((a/250)*c));

    y = max((3*b/10),b-((b/250)*d));

    if(x<y)
    {
        cout<<"Vasya\n";
    }
    else if(x>y)
    {
        cout<<"Misha\n";
    }
    else
    {
        cout<<"Tie\n";
    }

}

